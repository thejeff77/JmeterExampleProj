        <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="Print Output Assertion" enabled="true">
          <stringProp name="BeanShellAssertion.query">// This is executed after each test unless SampleResult is null.

red = &quot;\u001b[31m&quot;;
green = &quot;\u001b[32m&quot;;
color_reset = &quot;\u001b[0m&quot;;
result = SampleResult.isSuccessful() ? green + &quot;PASS&quot; : red + &quot;FAIL&quot;;
print(SampleResult.getSampleLabel() + &quot;, Result: &quot; + result + color_reset);

if (ResponseCode != null &amp;&amp; SampleResult.isSuccessful() == false )
{
	Failure=true ;
	FailureMessage = &quot;Test Failed an Assertion. The response code is: &quot; + ResponseCode + &quot;.&quot; ;
	print (&quot;===============FAILED_TEST DETAILED_INFORMATION (START)=================&quot;);
	print (SampleResult.getSampleLabel() + &quot;, HTTP Request URL: &quot; + red + SampleResult.getURL() + color_reset);  
	print (SampleResult.getSampleLabel() + &quot;, Request Headers: &quot; + red + RequestHeaders + color_reset);  
	print (SampleResult.getSampleLabel() + &quot;, Response Data: &quot; + red + new String(ResponseData) + color_reset);
	print (SampleResult.getSampleLabel() + &quot;, Response Code: &quot; + red + ResponseCode + color_reset + &quot;, Response Message: &quot; + red + ResponseMessage + color_reset + &quot;, Load Time: &quot; + red + SampleResult.getTime() + color_reset);   
	print (SampleResult.getSampleLabel()  + &quot;, Failure Message : &quot; + red + new String(FailureMessage) + color_reset);
	print(&quot;===============FAILED_TEST DETAILED_INFORMATION (END)==================&quot;);
}

addTo(varName, num)
{
	value = Integer.decode(vars.get(varName));
	value = value + num;
	vars.put(varName, value.toString());
}

addTo(&quot;TOTAL_TESTS&quot;, 1);
if (SampleResult.isSuccessful())
{
	addTo(&quot;TESTS_PASSED&quot;, 1);
}
else
{
	addTo(&quot;TESTS_FAILED&quot;, 1);
}

if (SampleResult.getSampleLabel().equals(&quot;Debug Sampler&quot;))
{
	System.out.println(&quot;Thread Group: Service Test&quot;);
	String date = vars.get(&quot;mydate&quot;);
	String starttime = vars.get(&quot;starttime&quot;);
	String endtime = vars.get(&quot;endtime&quot;);
     System.out.println(&quot;Date: &quot; + date + &quot;, Start time: &quot; + starttime + &quot;, End time: &quot; + endtime);
	
	results = &quot;Total tests: &quot; + vars.get(&quot;TOTAL_TESTS&quot;);
	results += &quot;   passed: &quot; + green + vars.get(&quot;TESTS_PASSED&quot;) + color_reset;
	if (vars.get(&quot;TESTS_FAILED&quot;).equals(&quot;0&quot;))
	{
		results += &quot;   failed: 0&quot;;
	}
	else
	{
		results += &quot;   failed: &quot; + red + vars.get(&quot;TESTS_FAILED&quot;) + color_reset;
	}
	print(results);

}
</stringProp>
