        <stringProp name="parameters"></stringProp>
        <stringProp name="filename"></stringProp>
        <stringProp name="script">// Use the -Jstack command line param as the host, (defaults to dev.example.com if unset)
vars.put(&quot;COMMON_HOST&quot;, &quot;${STACK}&quot;);

// Currently all environments are https, so use this globally as a default.
// __P Test Variables are set to be &quot;notset&quot; if they were not specified via the command line.
if(vars.get(&quot;COMMON_PORT&quot;).equals(&quot;notset&quot;) == true) {
	vars.put(&quot;COMMON_PORT&quot;, &quot;${PORT_HTTPS}&quot;);
}

if(vars.get(&quot;COMMON_PROTOCOL&quot;).equals(&quot;notset&quot;) == true) {
	vars.put(&quot;COMMON_PROTOCOL&quot;, &quot;${PROTOCOL_HTTPS}&quot;);
}
</stringProp>
