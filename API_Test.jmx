        <stringProp name="script">// Example pre-script setup &amp; verification - this one checks username/password.

boolean setCorrectly = true;

if(true == vars.get(&quot;COMMON_API_USERNAME&quot;).equals(&quot;notset&quot;))
{
	print (&quot;API Username not set, this is required, exiting.&quot;);
	setCorrectly = false;
}
if(true == vars.get(&quot;COMMON_API_PASSWORD&quot;).equals(&quot;notset&quot;))
{
	print (&quot;API Password not set, this is required, exiting.&quot;);
	setCorrectly = false;
}

// Example enforcement of input variables &amp; quitting
if (false == setCorrectly)
{
     // org.apache.jmeter.engine.StandardJMeterEngine.stopThread(Thread.currentThread().getName()); 
     // print (&quot;Stopped the thread from pre processor&quot;);

     // Uncomment the above lines to quit if username/password are not set via command line.
     print (&quot;Warn: username and password have not been set.&quot;);
}



</stringProp>
